{
  "name": "fepper",
  "description": "A Front End Prototyper tool for rapid prototyping of web sites",
  "main": "core/fepper.js",
  "scripts": {
    "prepush": "npm test",
    "test:eslint:extend": "npx eslint excludes/extend/*.js && npx eslint excludes/extend/*/*.js && npx eslint excludes/extend/*/*/*.js",
    "test:eslint:fepper": "npx eslint core/**/*.js",
    "test:eslint:tasker": "npx eslint tasker/**/*.js",
    "test:eslint:root": "npx eslint *.js",
    "test:eslint:test": "npx eslint test",
    "test:eslint:ui": "npx eslint ui/**/*.js",
    "test:eslint": "npm run test:eslint:extend && npm run test:eslint:fepper && npm run test:eslint:tasker && npm run test:eslint:root && npm run test:eslint:test && npm run test:eslint:ui",
    "test:mocha:core": "npx mocha test/tests/*.js",
    "test:mocha:ui": "npx mocha ui/test/tests/*.js",
    "test:mocha": "npm run test:mocha:core && npm run test:mocha:ui",
    "test": "npm run test:eslint && npm run test:mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/fepper-npm.git"
  },
  "keywords": [
    "atomic",
    "component",
    "design",
    "end",
    "fepper",
    "front",
    "front-end",
    "frontend",
    "lab",
    "library",
    "pattern",
    "prototype",
    "prototyper",
    "prototyping",
    "template"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/fepper-npm/issues"
  },
  "homepage": "http://fepper.io/",
  "engines": {
    "node": ">=8.5.0"
  },
  "dependencies": {
    "body-parser": "^1.6.3",
    "chalk": "^0.5.1",
    "cookie-parser": "^1.4.3",
    "diveSync": "^0.3.0",
    "event-stream": "^3.1.7",
    "express": "^4.16.3",
    "faye-websocket": "^0.11.1",
    "feplet": "^0.2.0",
    "fepper-utils": "^1.1.1",
    "fs-extra": "^7.0.0",
    "gray-matter": "^3.1.1",
    "he": "1.1.0",
    "html2json": "^1.0.2",
    "js-beautify": "1.6.4",
    "js-yaml": "^3.12.0",
    "json5": "0.5.1",
    "livereload-js": "^2.2.0",
    "marked": "^0.4.0",
    "parseurl": "^1.3.2",
    "qs": "^6.5.2",
    "rcloader": "^0.2.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "request": "^2.40.0",
    "slash": "^2.0.0"
  },
  "devDependencies": {
    "chai": "4.x",
    "fepper-ui": "^0.4.1",
    "eslint": "3.x",
    "mocha": "5.x",
    "husky": "^0.14.3",
    "json-eval": "^1.0.2"
  }
}
